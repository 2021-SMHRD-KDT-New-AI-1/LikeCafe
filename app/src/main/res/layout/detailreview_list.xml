<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="138dp" />

    <ImageView
        android:id="@+id/img_reviewDetail"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cafeimagexml" />

    <TextView
        android:id="@+id/tv_reviewDetailName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="닉네임"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:layout_constraintTop_toTopOf="@+id/img_reviewDetail" />

    <TextView
        android:id="@+id/tv_reviewDetailContent"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="15dp"
        android:text="dhdhdhdddddddddddddddddddddddddddddd"
        app:layout_constraintBottom_toBottomOf="@+id/img_reviewDetail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_reviewDetailName"
        app:layout_constraintTop_toBottomOf="@+id/tv_reviewDetailName" />

    <ImageView
        android:id="@+id/img_detailStar1"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_reviewDetailName"
        app:layout_constraintStart_toEndOf="@+id/guideline11"
        app:layout_constraintTop_toTopOf="@+id/tv_reviewDetailName"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/img_detailStar2"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_detailStar1"
        app:layout_constraintStart_toEndOf="@+id/img_detailStar1"
        app:layout_constraintTop_toTopOf="@+id/img_detailStar1"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/img_detailStar3"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_detailStar2"
        app:layout_constraintStart_toEndOf="@+id/img_detailStar2"
        app:layout_constraintTop_toTopOf="@+id/img_detailStar2"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/img_detailStar4"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_detailStar3"
        app:layout_constraintStart_toEndOf="@+id/img_detailStar3"
        app:layout_constraintTop_toTopOf="@+id/img_detailStar3"
        app:srcCompat="@drawable/star" />

    <ImageView
        android:id="@+id/img_detailStar5"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_detailStar4"
        app:layout_constraintStart_toEndOf="@+id/img_detailStar4"
        app:layout_constraintTop_toTopOf="@+id/img_detailStar4"
        app:srcCompat="@drawable/star" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="322dp" />
</androidx.constraintlayout.widget.ConstraintLayout>