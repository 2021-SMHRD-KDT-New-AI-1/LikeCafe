<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00ff0000"
    tools:context=".r_selection">

    <Button
        android:id="@+id/button33"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="경북"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/button32"
        app:layout_constraintStart_toStartOf="@+id/btn_r10"
        app:layout_constraintTop_toBottomOf="@+id/btn_r10" />

    <Button
        android:id="@+id/btn_r8"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="인천"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r9"
        app:layout_constraintStart_toStartOf="@+id/brn_r7"
        app:layout_constraintTop_toBottomOf="@+id/brn_r7" />

    <Button
        android:id="@+id/btn_r10"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="제주"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/button33"
        app:layout_constraintStart_toStartOf="@+id/btn_r9"
        app:layout_constraintTop_toBottomOf="@+id/btn_r9" />

    <Button
        android:id="@+id/button32"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="경남"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/button33"
        app:layout_constraintTop_toBottomOf="@+id/button33" />

    <Button
        android:id="@+id/btn_r9"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="광주"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r10"
        app:layout_constraintStart_toStartOf="@+id/btn_r8"
        app:layout_constraintTop_toBottomOf="@+id/btn_r8" />

    <Button
        android:id="@+id/brn_r7"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="충북"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r8"
        app:layout_constraintStart_toStartOf="@+id/btn_r6"
        app:layout_constraintTop_toBottomOf="@+id/btn_r6" />

    <Button
        android:id="@+id/btn_r6"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="세종"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/brn_r7"
        app:layout_constraintStart_toStartOf="@+id/btn_r5"
        app:layout_constraintTop_toBottomOf="@+id/btn_r5" />

    <Button
        android:id="@+id/btn_r5"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="충남"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r6"
        app:layout_constraintStart_toStartOf="@+id/btn_r4"
        app:layout_constraintTop_toBottomOf="@+id/btn_r4" />

    <Button
        android:id="@+id/btn_r4"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="대전"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r5"
        app:layout_constraintStart_toStartOf="@+id/btn_r3"
        app:layout_constraintTop_toBottomOf="@+id/btn_r3" />

    <Button
        android:id="@+id/btn_r1"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="서울"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed" />

    <Button
        android:id="@+id/btn_r3"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="강원"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r4"
        app:layout_constraintStart_toStartOf="@+id/btn_r2"
        app:layout_constraintTop_toBottomOf="@+id/btn_r2" />

    <Button
        android:id="@+id/btn_r2"
        android:layout_width="114dp"
        android:layout_height="50dp"
        android:backgroundTint="#00ff0000"
        android:drawableTint="#00ff0000"
        android:text="경기"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#5D5D5D"
        app:layout_constraintBottom_toTopOf="@+id/btn_r3"
        app:layout_constraintStart_toStartOf="@+id/btn_r1"
        app:layout_constraintTop_toBottomOf="@+id/btn_r1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="118dp" />

    <FrameLayout
        android:id="@+id/fl"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="parent">


    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>